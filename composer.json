{
    "name": "pagely/structure",
    "description": "Simple, immutable data structures",
    "keywords": [
        "data structure",
        "immutable",
        "hash map",
        "hash table",
        "dictionary",
        "ordered list",
        "unordered list",
        "list",
        "set"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Equip Contributors",
            "homepage": "https://github.com/equip"
        },
        {
            "name": "Pagely",
            "homepage": "https://github.com/pagely"
        }
    ],
    "require": {
        "php": "^7.3 | ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "replace": {
        "equip/structure": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Equip\\Structure\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Equip\\Structure\\": "tests"
        }
    }
}
